remote$Total <- remote$X <- NULL # Remove unrequired variables
nrows <- nrow(remote)
remote <- remote[-c(1, nrows-3, nrows-2, nrows-1, nrows), ] # Remove unrequired rows
colnames(remote)[1] <- "SA2"
remote$Major.Cities.of.Australia..NSW. <- as.integer(as.character(remote$Major.Cities.of.Australia..NSW.))
remote$SA2 <- as.character(remote$SA2)
remote <- merge(sa2Data[c("SA2", "State")], remote, by="SA2") # Add state SA2 region belongs to
# Create dummy categories
columnnames <- names(remote[-1:-2])
#remote$MajorCities <- t(vapply("Major.Cities", "Regional", "Remote", "Other"), function(x) names(remote)[-1:-2] %in% x, logical(ncol(df)-1))
remote$nonzeros <- simplify2array(
apply(
remote[-1:-2], 1,
function(x) paste(names(remote[-1:-2])[x != 0], collapse = " ")
)
)
remote$City <- 0
remote$InnerRegional <- 0
remote$OuterRegional <- 0
remote$Remote <- 0
remote$VeryRemote <- 0
remote$MigratoryOffshoreShipping <- 0
remote$NoUsualAddress <- 0
remote$City[grepl("Cities", remote$nonzeros)] <- 1
remote$InnerRegional[grepl("Inner.Regional", remote$nonzeros)] <- 1
remote$OuterRegional[grepl("Outer.Regional", remote$nonzeros)] <- 1
remote$Remote[grepl("Remote.Australia", remote$nonzeros)] <- 1
remote$VeryRemote[grepl("Very.Remote", remote$nonzeros)] <- 1
remote$MigratoryOffshoreShipping[grepl("Migratory", remote$nonzeros)] <- 1
remote$NoUsualAddress[grepl("No.usual.address", remote$nonzeros)] <- 1
remote$nonzeros <- NULL
remote <- remote[c(1:2, 56:62)]
